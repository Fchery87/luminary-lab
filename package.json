{
  "name": "luminary-lab",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "bun --bun next dev --webpack",
    "build": "bun --bun next build --webpack",
    "start": "bun --bun next start",
    "lint": "bunx eslint .",
    "db:generate": "bunx drizzle-kit generate",
    "db:push": "bunx drizzle-kit push",
    "db:studio": "bunx drizzle-kit studio",
    "db:migrate": "bunx drizzle-kit migrate",
    "db:seed": "bun --bun src/db/seed.ts",
    "db:migrate-seed": "bunx drizzle-kit migrate && bun --bun src/db/seed.ts",
    "test": "bun test",
    "test:e2e": "bunx playwright test",
    "r2:verify": "bun --bun scripts/verify-r2-config.ts",
    "r2:migrate": "bun --bun scripts/migrate-s3-to-r2.ts",
    "r2:migrate:dry-run": "DRY_RUN=true bun --bun scripts/migrate-s3-to-r2.ts",
    "r2:verify-migration": "bun --bun scripts/migrate-s3-to-r2.ts '' verify"
  },
