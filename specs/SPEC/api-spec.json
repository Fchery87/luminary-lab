    "/admin/presets/{presetId}": {
      "put": {
        "tags": ["admin"],
        "summary": "Update preset (admin only)",
        "operationId": "updatePreset",
        "security": [{"bearerAuth": []}],
        "parameters": [
          {
            "name": "presetId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UpdatePresetRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Preset updated",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Preset"
